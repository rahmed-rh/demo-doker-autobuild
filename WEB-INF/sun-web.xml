<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Sun ONE Application Server 7.0 Servlet 2.3//EN" "http://www.sun.com/software/sunone/appserver/dtds/sun-web-app_2_3-1.dtd">

<sun-web-app>
  <session-config>
    <session-manager persistence-type="ha">
      <manager-properties>
        <property name="persistenceFrequency" value="web-method"/>
      </manager-properties>
      <store-properties>
        <property name="persistenceScope" value="session"/>
      </store-properties>
    </session-manager>
    <session-properties/>
    <cookie-properties/>
  </session-config>
  <cache max-entries="4096" timeout-in-seconds="30" enabled="false">
    <default-helper/>
  </cache>
  <jsp-config>
    <property value="true" name="classdebuginfo">
      <description>Enable debug info compilation in the generated servlet class</description>
    </property>
    <property value="true" name="mappedfile">
      <description>Maintain a one-to-one correspondence between static content and the generated servlet class' java code</description>
    </property>
  </jsp-config>
</sun-web-app>
